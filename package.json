{
	"name": "optimus",
	"displayName": "optimus",
	"description": "Optimus provides a simple way of live transforming data format to data format",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.optimus"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.optimus",
				"title": "Optimus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "*",
		"@types/mocha": "*",
		"@types/node": "*",
		"@types/vscode": "*",
		"@types/js-yaml": "*",
		"glob": "*",
		"mocha": "*",
		"tslint": "*",
		"typescript": "*",
		"vscode-test": "*"
	},
	"dependencies": {
		"js-yaml": "*"
	}
}
